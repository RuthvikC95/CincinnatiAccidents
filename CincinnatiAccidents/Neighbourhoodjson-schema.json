{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "array",
  "items": {
    "$ref": "#/definitions/NeighbourhoodElement"
  },
  "definitions": {
    "NeighbourhoodElement": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "address": {
          "type": "string"
        },
        "neighborhood": {
          "$ref": "#/definitions/Neighborhood"
        },
        "accidentDate": {
          "type": "string",
          "format": "date-time"
        },
        "latitude": {
          "type": "string"
        },
        "longitude": {
          "type": "string"
        },
        "accidentType": {
          "$ref": "#/definitions/AccidentType"
        }
      },
      "required": [
        "accidentDate",
        "accidentType",
        "address",
        "latitude",
        "longitude",
        "neighborhood"
      ],
      "title": "NeighbourhoodElement"
    },
    "AccidentType": {
